{
  "name": "nazg/skeleton",
  "description": "Action-Domain-Responder (ADR) Micro framework for HHVM/Hack",
  "type": "project",
  "keywords": [
    "hhvm",
    "hack",
    "framework",
    "adr",
    "middleware",
    "request",
    "response",
    "route",
    "xhp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "hhvm": "^4.15",
    "hhvm/hsl": "^4.7",
    "hhvm/hsl-experimental": "^4.7",
    "hhvm/hhvm-autoload": "^2.0.7",
    "nazg/framework": "^0.5.1",
    "ytake/hh-config-aggregator": "^0.5.0",
    "ytake/hackdotenv": "^0.3.0",
    "facebook/xhp-lib": "^2.9.4",
    "facebook/fbmarkdown": "^1.6"
  },
  "require-dev": {
    "hhvm/hacktest": "^1.6.1",
    "facebook/fbexpect": "^2.6.0",
    "hhvm/hhast": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
